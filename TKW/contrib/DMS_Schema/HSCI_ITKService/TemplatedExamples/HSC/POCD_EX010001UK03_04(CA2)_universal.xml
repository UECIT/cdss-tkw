                              <section3 moodCode="EVN" classCode="DOCSECT">
                                 <id root="7D5FE1A0-AE1A-11DB-96EF-B18E1E0994CD"/>
                                 <templateId root="2.16.840.1.113883.2.1.3.2.4.18.2" extension="COCD_TP146061UK02#section3"/>
                                 <text>
                                    <content ID="a1">CVA 6 months ago</content>
                                    <br/>
                                    <content ID="a2">Hypertension</content>
                                    <br/>
                                    <content ID="a3">Current chest infection</content>- recently had antibiotics (penicillin) - stopped 29.7.07
												<content ID="a4">due to rash.</content>
                                    <br/>
                                    <content ID="a5">Current asthma</content>
                                 </text>
                                 <title>Medical Conditions and Diagnoses</title>
                              </section3>
                           </component2>
                           <component2 typeCode="COMP" contextConductionInd="true">
                              <templateId root="2.16.840.1.113883.2.1.3.2.4.18.2"
                                          extension="COCD_TP146061UK02#component2"/>
                              <section3 moodCode="EVN" classCode="DOCSECT">
                                 <id root="7D5FE1A0-AE1A-11DB-96F0-B18E1E0994CD"/>
                                 <templateId root="2.16.840.1.113883.2.1.3.2.4.18.2" extension="COCD_TP146061UK02#section3"/>
                                 <text>Prednisolone starting 24.5.07
												<br/>Aspirin 75 mgs mane
												<br/>Latanaprost 50mgs/ml nocte
												<br/>Beclamethazone inhaler BD
												<br/>Citalopram 20 mgs mane
												<br/>Nifedipine MR 20 mgs BD
												<br/>Salbutamol inhaler QDS
												<br/>Simvastatin 20 mgs nocte
												<br/>Cetirizine 10mgs mane
												<br/>Brimonidine eye drops BD
												<br/>
                                 </text>
                                 <title>Current Medications</title>
                              </section3>
                           </component2>
                        </section2>
